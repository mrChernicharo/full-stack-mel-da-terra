{
  "compilerOptions": {
    "module": "commonjs",
    "removeComments": true,
    "skipLibCheck": true,
    "lib": ["es2021"],
    "target": "es2021",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "outDir": "./dist"
  },
  "exclude": ["node_modules", "dist"]
}
